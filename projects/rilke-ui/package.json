{
	"name": "@kantist/rilke-ui",
	"version": "0.0.21",
	"description": "Rilke UI Library",
	"keywords": [
		"angular",
		"ui",
		"material design",
		"components",
		"rilke",
		"rilke architecture"
	],
	"license": "MIT",
	"scripts": {
		"build": "tsc -p tsconfig.schematics.json",
