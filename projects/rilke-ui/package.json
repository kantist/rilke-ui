	},
	"peerDependencies": {
		"@angular/cdk": "^13.0.2",
		"@angular/core": "^13.0.0 || ^14.0.0-0",
		"@angular/common": "^13.0.0 || ^14.0.0-0",
		"@angular/material": "^13.0.2",
		"ngx-color": "^7.3.3",
		"tslib": "^2.3.0"
	},
	"devDependencies": {
		"@angular/cdk": "^13.0.2",
		"@angular/core": "^13.0.0 || ^14.0.0-0",
		"@angular/common": "^13.0.0 || ^14.0.0-0",
		"@angular/material": "^13.0.2",
		"ngx-color": "^7.3.3",
		"tslib": "^2.3.0",
		"copyfiles": "file:../../node_modules/copyfiles",
		"typescript": "file:../../node_modules/typescript"
	},
	"schematics": "./schematics/collection.json",
	"ng-add": {}
}
